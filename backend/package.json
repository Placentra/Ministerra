{
	"name": "backend",
	"version": "1.0.0",
	"description": "",
	"main": "app.ts",
	"type": "module",
	"scripts": {
		"lint": "eslint .",
		"lint:fix": "eslint . --fix",
		"logs": "npx --yes tsx@4.7.0 scripts/log-viewer.ts",
		"start": "TZ=UTC npx --yes tsx@4.7.0 --watch app.ts",
		"debug": "TZ=UTC npx --yes tsx@4.7.0 --watch --inspect app.ts",
		"down": "npx --yes tsx@4.7.0 scripts/docker-clean.ts docker-compose.yml ministerra",
		"nuke": "npx --yes tsx@4.7.0 scripts/docker-clean.ts docker-compose.yml ministerra --prune-volumes",
		"dev-": "npm run down && docker compose -p ministerra -f docker-compose.yml --profile dev up",
		"dev": "docker compose -p ministerra -f docker-compose.yml restart backend-app frontend-dev && docker compose -p ministerra -f docker-compose.yml logs -f backend-app frontend-dev",
		"min-": "npm run down && docker compose -p ministerra -f docker-compose.yml up -d redis-primary mysql && docker compose -p ministerra -f docker-compose.yml --profile min up -d backend-min frontend-dev && docker compose -p ministerra -f docker-compose.yml logs -f backend-min frontend-dev",
		"min": "docker compose -p ministerra -f docker-compose.yml restart backend-min frontend-dev && docker compose -p ministerra -f docker-compose.yml logs -f backend-min frontend-dev",
		"prod": "docker compose -p ministerra --profile prod up -d --build"
	},
	"keywords": [],
	"author": "",
	"license": "ISC",
